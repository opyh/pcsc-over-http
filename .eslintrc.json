{
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true
  },
  "plugins": [
    "flowtype"
  ],
  "env": {
    "node": true
  },
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-console": [0],
    "no-underscore-dangle": [0],
    "import/no-extraneous-dependencies": [0],
    "import/extensions": [0],
    "class-methods-use-this": [0],
    "object-shorthand": [0],
    "no-bitwise": [0]
  },
  "globals": {
  }
}
